<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dcstress.updateupgrade.dc_stress">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dcstress.updateupgrade.dc_stress.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><![CDATA[
        />
        ]]>
        <activity
            android:name="com.dcstress.updateupgrade.dc_stress.Ejercicios"
            android:label="@string/title_activity_ejercicios"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dcstress.updateupgrade.dc_stress.Meditacion"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>/>
        <activity android:name="com.dcstress.updateupgrade.dc_stress.creditos"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>/>
        <activity android:name="com.dcstress.updateupgrade.dc_stress.Rep_musica"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>/>

        <service
            android:name="com.dcstress.updateupgrade.dc_stress.Service.MyFirebaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service android:name=".Service.MyFirebaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>



    </application>

</manifest>